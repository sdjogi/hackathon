<?xml version="1.0" encoding="UTF-8"?>
<faces-config version="2.1" xmlns="http://java.sun.com/xml/ns/javaee"
 xmlns:xi="http://www.w3.org/2001/XInclude"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_1.xsd">
 <navigation-rule>
  <display-name>newfile</display-name>
  <from-view-id>/NewFile.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>failed</from-outcome>
   <to-view-id>/Failed.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>welcome</from-outcome>
   <to-view-id>/welcome.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
</faces-config>
